{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": ""
  },
  "Discord": {
    "Enabled": true,
    "ChannelTag": "ALERTS",
    "WebhookUrl": ""
  },
  "Preview": {
    "Enabled": false,
    "MinScore": 5.0,
    "MaxSignalsPerMinute": 30,
    "PerSymbolCooldownSeconds": 0,
    "RequireBookValid": true,
    "RequireTapeRecent": false,
    "DiscordEnabled": true,
    "DiscordChannelTag": "PREVIEW"
  },
  "RecordBlueprints": true,
  "ShadowTrading": {
    "GatingRejectSuppressionSeconds": 10
  },
  "ShadowTradeJournal": {
    "FilePath": "logs/shadow-trade-journal.jsonl",
    "EmitGateTrace": true
  },
  "Report": {
    "DailyRollup": false,
    "JournalPath": "",
    "WriteToFile": false,
    "OutputPath": ""
  },
  "Diagnostics": {},
  "Scarcity": {
    "MaxBlueprintsPerDay": 6,
    "MaxPerSymbolPerDay": 1,
    "GlobalCooldownMinutes": 45,
    "SymbolCooldownMinutes": 999,
    "RankWindowSeconds": 0
  },
  "Universe": {
    "Source": "IbkrScanner",
    "StaticTickers": [
      "AAPL",
      "MSFT",
      "GOOGL",
      "AMZN"
    ],
    "ExcludeEtfs": true,
    "EtfDenylist": [
      "TZA",
      "DUST",
      "BITO",
      "TSLL"
    ],
    "Scanner": {
      "MaxRetries": 3
    },
    "IbkrScanner": {
      "Instrument": "STK",
      "LocationCode": "STK.US.MAJOR",
      "ScanCodes": [
        "TOP_PERC_GAIN",
        "HOT_BY_VOLUME",
        "MOST_ACTIVE"
      ],
      "Rows": 50,
      "AbovePrice": 10,
      "BelowPrice": 80,
      "AboveVolume": 1000000,
      "FloatSharesBelow": 50000000
    },
    "MaxActiveSymbols": 25,
    "MinPrice": 5,
    "MaxPrice": 80,
    "MaxSpread": 0.05,
    "MaxTickers": 50,
    "UseStaticTickers": false,
    "RebuildIntervalHours": 4
  },
  "IBKR": {
    "Enabled": true,
    "Host": "127.0.0.1",
    "Port": 7496,
    "ClientId": 1
  },
  "MarketData": {
    "MaxLines": 80,
    "EnableDepth": true,
    "EnableTape": true,
    "MinHoldMinutes": 5,
    "TickByTickMaxSymbols": 6
  },
  "Execution": {
    "Enabled": true,
    "Live": false,
    "Ledger": {
      "Type": "Jsonl",
      "FilePath": "logs/execution-ledger.jsonl"
    },
    "AutoExecuteFromSignals": false,
    "AutoExecuteNotionalUsd": 500,
    "KillSwitch": false,
    "MaxOrdersPerDay": 20,
    "MaxBracketsPerDay": 10,
    "MaxOpenPositions": 3,
    "MaxNotionalPerTradePct": 10,
    "MaxLossPerDayUsd": 200,
    "SymbolCooldownSeconds": 120,
    "MinSecondsBetweenOrders": 10,
    "Broker": "Fake",
    "MaxNotionalUsd": 2000,
    "MaxShares": 500
  }
}
